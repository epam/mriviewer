{
  "name": "mri-viewer",
  "fullName": "MRI Viewer 2d/3d Dicom and other medical formats web viewer/editor",
  "description": "MRI Viewer is unique 2d/3d medical volume data web viewer application. You can inspect data both in 2d and 3d modes.",
  "version": "1.0.2",
  "author": "EPAM Systems <mri@epam.com>",
  "year": "2022",
  "license": "Apache-2.0",
  "proxy": "http://localhost:3000",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "https://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "bugs": {
    "url": "https://github.com/epam/mriviewer/issues"
  },
  "homepage": "https://mriviewer.opensource.epam.com/",
  "private": false,
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.0.6",
    "@popperjs/core": "^2.11.6",
    "@tensorflow/tfjs": "^3.11.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "classnames": "^2.3.1",
    "daikon": "^1.2.42",
    "eslint": "^7.32.0",
    "filereader-stream": "^2.0.0",
    "pako": "^2.1.0",
    "process": "^0.11.10",
    "react": "^17.0.2",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.1",
    "react-dom": "^17.0.2",
    "react-modal": "^3.14.4",
    "react-popper": "2.2.5",
    "react-redux": "^7.2.6",
    "react-scripts": "5.0.0",
    "readable-stream": "^4.3.0",
    "redux": "^4.1.2",
    "stream-browserify": "^3.0.0",
    "three": "^0.101.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.x.x",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.4.1",
    "react-app-rewired": "^2.2.1",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --detectOpenHandles",
    "debugtest": "DEBUG=true react-app-rewired test",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.{js,jsx,html,css}\"",
    "docker": "npm run build && docker run --name mri-viewer -v ./build:/usr/share/nginx/html:ro -d -p 8080:80 nginx",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "resetMocks": false,
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary",
      "clover"
    ],
    "collectCoverageFrom": [
      "./src/**/*.{js,jsx,ts,tsx}",
      "!./node_modules/",
      "!./build/"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie > 1",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  }
}
